<iframe src="https://badbook.whereby.com/demo-d08ac869-1607-4582-ac53-a6aea20204e9" allow="camera; microphone; fullscreen; speaker; display-capture; compute-pressure" style="height: 700px; width: 100%"></iframe>

<% @bookshelves.each do |bookshelf|  %>
  <% unless bookshelf.name == "Read Books" %>
    <div id="booksCarousel" class="carousel slide mx-auto" data-bs-ride="false">
      <h2 class="carousel-header"><%= bookshelf.name.gsub("_"," ").titleize %></h2>
      <%= render 'carousel', books: bookshelf.books %>
      <button class="carousel-control-prev" type="button" data-bs-target="#booksCarousel" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      </button>
      <button class="carousel-control-next" type="button" data-bs-target="#booksCarousel" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
      </button>
    </div>
  <% end %>
<% end %>
